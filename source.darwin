# source this file to setup the iotivity build environment on darwin

export IOTIVITY_HOST_OS=darwin
export IOTIVITY_HOST_ARCH=x86_64

export IOTIVITY_TARGET_OS=darwin
export IOTIVITY_TARGET_ARCH=x86_64

export IOTIVITY_STAGE=release

export IOTIVITY_HOME=$HOME/iotivity/iotk/iochibity
export IOTIVITY_LIBPATH=${IOTIVITY_HOME}/out/${IOTIVITY_HOST_OS}/${IOTIVITY_TARGET_ARCH}/${IOTIVITY_STAGE}
